    "tsx": "4.20.6",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "axios": "1.13.1",
    "express": "5.1.0",
    "http-php": "1.3.4",
    "http-proxy": "1.18.1",
    "nodemailer": "7.0.10",
    "php": "1.1.0",
    "php-express": "0.0.3"
