{
  "name": "@webpage/frontend",
  "author": "Sean McBroom (sean.mcbroom@outlook.com)",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": "./src/main.ts"
  },
  "scripts": {
    "analyze:bundle": "vite build --mode analyze:bundle",
    "analyze:cycles": "vite build --mode analyze:cycles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "eslint-config-inspector",
    "build": "vite build",
    "check": "npm run lint && npm run check:types",
    "check:types": "vue-tsc",
    "start": "vite",
    "serve": "vite preview",
    "dev": "npm run start",
    "prod": "npm run build && npm run serve",
    "clean": "git clean -fxd"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@vueuse/core": "10.11.0",
    "axios": "1.6.8",
    "defu": "6.1.4",
    "vue": "3.4.21",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "20.1.4",
    "@types/node": "20.12.11",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.19",
    "file-loader": "6.2.0",
    "globals": "16.0.0",
    "npm-run-all2": "6.1.2",
    "postcss": "8.4.39",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.5",
    "unplugin-vue-components": "0.27.2",
    "url-loader": "4.1.1",
    "vite": "5.2.8",
    "vite-plugin-vue-devtools": "7.7.1"
  }
}
