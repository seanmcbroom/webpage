{
  "name": "@webpage/frontend",
  "author": "Sean McBroom (sean.mcbroom@outlook.com)",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": "./src/main.ts"
  },
  "scripts": {
    "analyze:bundle": "vite build --mode analyze:bundle",
    "analyze:cycles": "vite build --mode analyze:cycles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "eslint-config-inspector",
    "build": "vite build && tsx scripts/generateSitemap.ts",
    "check": "npm run lint && npm run check:types",
    "check:types": "vue-tsc",
    "start": "vite",
    "serve": "vite preview",
    "dev": "npm run start",
    "prod": "npm run build && npm run serve",
    "clean": "git clean -fxd"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "6.0.8",
    "@tailwindcss/vite": "4.1.16",
    "@vueuse/core": "13.9.0",
    "axios": "1.13.2",
    "defu": "6.1.4",
    "markdown-it": "14.1.0",
    "vue": "3.5.24",
    "vue-i18n": "11.1.12",
    "vue-router": "4.6.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@tsconfig/node20": "20.1.6",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "autoprefixer": "10.4.22",
    "file-loader": "6.2.0",
    "globals": "16.5.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.17",
    "unplugin-vue-components": "30.0.0",
    "url-loader": "4.1.1",
    "vite": "7.1.12",
    "vite-imagetools": "^9.0.0",
    "vite-plugin-vue-devtools": "8.0.5",
    "vue-eslint-parser": "^10.2.0"
  }
}
